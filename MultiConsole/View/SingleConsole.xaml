<UserControl x:Class="MultiConsole.View.SingleConsole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MultiConsole.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="{Binding FileName}" Margin="2" Padding="2" Background="#4FFF" Foreground="#FFF"/>
        <ScrollViewer Margin="2" Padding="2" Background="#2FFF" Foreground="#FFF" Height="80"  >
            <TextBlock Text="{Binding Outputs}" />
            
        </ScrollViewer>
    </StackPanel>
</UserControl>
